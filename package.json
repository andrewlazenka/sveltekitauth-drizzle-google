{
	"name": "sveltekitauth-drizzle-google",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"db:migrate": "wrangler d1 execute sveltekitauth-drizzle-google --local --file=./migrations/0000_lonely_saracen.sql",
		"deploy": "npm run build && wrangler pages deploy --env production --project-name sveltekitauth-drizzle-google .svelte-kit/cloudflare",
		"dev": "vite dev",
		"build": "vite build",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --plugin-search-dir . --write .",
		"drizzle:generate": "drizzle-kit generate:sqlite",
		"drizzle:up": "drizzle-kit up:sqlite",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"preview": "vite preview",
		"watch": "wrangler pages dev .svelte-kit/cloudflare"
	},
	"devDependencies": {
		"@sveltejs/adapter-cloudflare": "^2.3.3",
		"@sveltejs/kit": "^1.27.1",
		"@types/lodash.kebabcase": "^4.1.8",
		"@types/lodash.startcase": "^4.4.8",
		"@types/uuid": "^9.0.6",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"better-sqlite3": "^9.0.0",
		"drizzle-kit": "^0.19.13",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"postcss": "^8.4.31",
		"prettier": "^2.8.8",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.2.2",
		"svelte-check": "^3.5.2",
		"tailwindcss": "^3.3.5",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.5.0",
		"wrangler": "^3.1.2"
	},
	"type": "module",
	"dependencies": {
		"@auth/core": "^0.18.0",
		"@auth/drizzle-adapter": "^0.3.5",
		"@auth/sveltekit": "^0.3.11",
		"@cloudflare/workers-types": "^4.20231025.0",
		"drizzle-orm": "^0.28.6",
		"lodash.kebabcase": "^4.1.1",
		"lodash.startcase": "^4.4.0",
		"uuid": "^9.0.1"
	}
}
